{
  "name": "psychic-octo-doodle",
  "version": "1.0.0",
  "description": "",
  "main": "dist/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "prettier": "npx prettier --write ./src",
    "fix:code": "npx eslint --fix --ignore-path .gitignore --ignore-path .prettierignore --ext .ts .",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/r-amos/psychic-octo-doodle.git"
  },
  "author": "Robert Amos",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/r-amos/psychic-octo-doodle/issues"
  },
  "type": "module",
  "homepage": "https://github.com/r-amos/psychic-octo-doodle#readme",
  "dependencies": {
    "express": "^4.18.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.16.3",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "concurrently": "^8.0.1",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "husky": "^8.0.3",
    "lint-staged": "13.2.2",
    "nodemon": "^2.0.22",
    "prettier": "2.8.8",
    "typescript": "^5.0.4"
  },
  "lint-staged": {
    "**./*.ts": [
      "npx eslint --fix --ext .ts .",
      "prettier --write ./src"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
